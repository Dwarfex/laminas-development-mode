{
    "name": "laminas/laminas-development-mode",
    "description": "Laminas development mode script",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "framework"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-development-mode/",
        "issues": "https://github.com/laminas/laminas-development-mode/issues",
        "source": "https://github.com/laminas/laminas-development-mode",
        "rss": "https://github.com/laminas/laminas-development-mode/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev",
            "dev-develop": "3.2-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\DevelopmentMode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\DevelopmentMode\\": "test/"
        }
    },
    "bin": [
        "bin/laminas-development-mode"
    ],
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "coveralls": "coveralls",
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    },
    "replace": {
        "zfcampus/zf-development-mode": "self.version"
    }
}
